<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title><%=title %></title>
</head>
<body>

    <%- include('partials/menu.ejs') %>

    <h1><%=title %></h1>
    <% if (emailsVisible) {%>
        <h3>Emails:</h3>
        <ul>
            <% for(var i=0; i<emails.length; i++) {%>
            <li><%=emails[i] %></li>
            <%} %>
        </ul>
    <% }else{%>
        <h3>Emails not found</h3>
    <%} %>
    <p>Phone: <%=phone %></p>

    <%- include('partials/footer.ejs') %>
</body>
</html>